      <div class="container-product">  
        <form id="contact" action="" method="post">
          <h3>Add Product</h3>
          <h4>Add your product's Details below</h4>
          <fieldset>
            <input placeholder="Product Name" type="text" tabindex="1" required autofocus>
          </fieldset>
          <fieldset>
            <input placeholder="Image URL" type="email" tabindex="2" required>
          </fieldset>
          <fieldset>
            <input placeholder="Price ( Rs.)" type="tel" tabindex="3" required>
          </fieldset>
          <fieldset>
            <input placeholder="Stock" type="url" tabindex="4" required>
          </fieldset>
          <fieldset>
            <textarea placeholder="Product's Description" tabindex="5" required></textarea>
          </fieldset>
              <fieldset>
            <input placeholder="Category" type="url" tabindex="6" required>
          </fieldset>
              <fieldset>
            <input placeholder="Discount" type="url" tabindex="7" required>
          </fieldset>
          <fieldset>
            <button name="submit" type="submit" id="contact-submit" data-submit="...Sending">Submit</button>
          </fieldset>
        </form>
       
        
      </div>
